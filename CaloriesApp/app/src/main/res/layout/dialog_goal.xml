<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginStart="12dp"
    android:layout_marginEnd="12dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/bg_white_corner_24"
    >


    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="16dp"
        android:paddingTop="12dp"
        android:text="Congratulations!"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView_namefood_dialogAddfood"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_namefood_dialogAddfood"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="32dp"
        android:text="Your custom plan is ready"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_dialogAddfood"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Your daily goal is:"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@+id/textView_namefood_dialogAddfood"
        app:layout_constraintTop_toBottomOf="@+id/textView_namefood_dialogAddfood" />


    <TextView
        android:id="@+id/textView_calodaily_dialogcongratulation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/textView_dialogAddfood"
        app:layout_constraintTop_toBottomOf="@+id/textView_dialogAddfood" />

    <TextView
        android:id="@+id/textView45"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="kcal"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView_calodaily_dialogcongratulation"
        app:layout_constraintStart_toEndOf="@+id/textView_calodaily_dialogcongratulation" />

    <TextView
        android:id="@+id/textViewkk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="In: "
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/textView_calodaily_dialogcongratulation"
        app:layout_constraintTop_toBottomOf="@+id/textView_calodaily_dialogcongratulation" />

    <TextView
        android:id="@+id/textView_time_dialogcongratulation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/textViewkk"
        app:layout_constraintTop_toBottomOf="@+id/textView_calodaily_dialogcongratulation" />

    <TextView
        android:id="@+id/textViewkk3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="Weeks"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView_time_dialogcongratulation"
        app:layout_constraintStart_toEndOf="@+id/textView_time_dialogcongratulation" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/start_congratulation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"

        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:backgroundTint="@color/blue_200"
        android:text="Get Started"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewkk"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>